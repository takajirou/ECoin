{
    "expo": {
        "name": "ECoin",
        "slug": "ECoin",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/coin.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "scheme": "frontend-scheme",
        "splash": {
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true
        },
        "web": {
            "bundler": "metro"
        },
        "plugins": ["expo-router"]
    }
}
